{
  "name": "nextjs-chakra-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ci": "npm run lint $(git diff --name-only --diff-filter=ACMRTUXB origin/main | grep  -E \"(.js$|.jsx$|.ts$|.tsx$)\") --quiet",
    "prettier:check": "npx prettier --check \"(.js,jsx,ts,tsx,css,scss,less,html,json,md$)\")",
    "prettier:write": "npx prettier --write \"(.js,jsx,ts,tsx,css,scss,less,html,json,md$)\")",
    "prettier:ci": "prettier --check $(git diff --name-only --diff-filter=ACMRTUXB origin/main | grep  -E \"(js,jsx,ts,tsx,css,scss,less,html,json,md$)\")"
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.17",
    "@chakra-ui/react": "^2.8.0",
    "@chakra-ui/theme-tools": "2.0.17",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@heroicons/react": "2.0.15",
    "axios": "1.3.3",
    "chakra-react-select": "^4.7.0",
    "dayjs": "1.11.8",
    "i18next": "22.5.1",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.2",
    "react-i18next": "12.3.1",
    "react-number-format": "^5.2.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.2.4",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  }
}
